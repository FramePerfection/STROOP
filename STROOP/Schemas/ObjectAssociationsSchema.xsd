<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="ObjectAssociationsSchema"
    elementFormDefault="qualified"
    xmlns="http://tempuri.org/ObjectAssociationsSchema.xsd"
    xmlns:mstns="http://tempuri.org/ObjectAssociationsSchema.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:types="http://tempuri.org/ReusableTypes.xsd"
>
  <xs:import namespace="http://tempuri.org/ReusableTypes.xsd" schemaLocation="ReusableTypes.xsd"/>
  <xs:element name ="ObjectAssociations">
    <xs:complexType>
      <xs:sequence>
	  
        <xs:element name="Config">
          <xs:complexType>
            <xs:all>
              <xs:element name="String">
                <xs:complexType>
                  <xs:attribute name="name" use="required" type="xs:string"/>
                </xs:complexType>
              </xs:element>
            </xs:all>
          </xs:complexType>
        </xs:element>
		
        <xs:element name="Mario">
          <xs:complexType>
            <xs:all>
              <xs:element name="Image" maxOccurs="1">
                <xs:complexType>
                  <xs:attribute name="path" use="required" type="xs:string"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="MapImage" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                  <xs:attribute name="path" use="required" type="xs:string"/>
                  <xs:attribute name="rotates" use="optional" type="xs:boolean"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="Color" type="types:color"/>
            </xs:all>
            <xs:attribute name="behaviorScriptAddress" use="required" type="types:address"/>
          </xs:complexType>
        </xs:element>
		
		<xs:element name="MapImage">
			<xs:complexType>
			  <xs:attribute name="name" use="required" type="xs:string"/>
			  <xs:attribute name="path" use="required" type="xs:string"/>
			</xs:complexType>
		</xs:element>
		
		<xs:element name="SomeStructYo">
			<xs:complexType>
				<xs:all>
				  <xs:element name="ImagePath" maxOccurs="1" type="xs:string"/>
				  <xs:element name="MapImagePath" maxOccurs="1" type="xs:string"/>
				  <xs:element name="Color" type="types:color"/>
				</xs:all>
				<xs:attribute name="identifier" use="required" type="xs:string"/>
			</xs:complexType>
		</xs:element>
		
        <xs:element name="Overlays">
          <xs:complexType>
            <xs:all>
              <xs:element name="Selected">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="TrackedAndShown">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="TrackedNotShown">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="StoodOn">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="Held">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="Interaction">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="Used">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="Camera">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="CameraHack">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="Model">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="Floor">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="Wall">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="Ceiling">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="Parent">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="ParentUnused">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="ParentNone">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="Marked">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="OverlayImage" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="path" use="required" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
            </xs:all>
          </xs:complexType>
        </xs:element>
        <xs:element name="Object" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Image">
                <xs:complexType>
                  <xs:attribute name="path" use="required" type="xs:string"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="MapImage" minOccurs="0">
                <xs:complexType>
                  <xs:attribute name="path" use="required" type="xs:string"/>
                  <xs:attribute name="rotates" use="optional" type="xs:boolean"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="Data" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="types:watch-variable">
                      <xs:attribute name="objectOffset" type="xs:boolean" use="optional"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="behaviorScriptAddress" use="required" type="types:address"/>
            <xs:attribute name="gfxId" use="optional" type="types:address"/>
            <xs:attribute name="subType" use="optional" type="xs:int"/>
            <xs:attribute name="appearance" use="optional" type="xs:int"/>
            <xs:attribute name="name" use="required" type="xs:string"/>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>